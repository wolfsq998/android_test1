<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@drawable/logoing">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="10dp"
        android:id="@+id/linearLayout4"
        android:background="#6d5a5a"
        android:visibility="gone">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="忘记密码"
            android:layout_weight="1"
            android:textColor="@color/colorWhite"
            android:layout_marginRight="1dp"
            android:textSize="12dp"
            android:background="@color/colorAlpha1"
            android:gravity="center|right"
            android:paddingRight="15dp"
            android:layout_marginLeft="20dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"></LinearLayout>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="立即注册"
            android:id="@+id/login_win_reg_but"
            android:layout_weight="1"
            android:textColor="@color/colorWhite"
            android:layout_marginLeft="1dp"
            android:textSize="12dp"
            android:background="@color/colorAlpha1"
            android:paddingLeft="15dp"
            android:gravity="left|center"
            android:layout_marginRight="20dp"
            android:enabled="false"
            android:clickable="false"/>
    </LinearLayout>

    <ImageView
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:id="@+id/imageView12"
        android:background="@drawable/icon_back2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/login_win_update_pas_but"
        android:background="@drawable/icon_button1"
        android:layout_alignBottom="@+id/imageView12"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12"
        android:visibility="gone" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="忘记\n密码"
        android:id="@+id/textView26"
        android:textSize="20dp"
        android:gravity="center"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12"
        android:layout_alignBottom="@+id/imageView12"
        android:textColor="@color/colorWhite"
        android:enabled="false"
        android:clickable="false"
        android:layout_alignTop="@+id/login_win_update_pas_but"
        android:layout_alignRight="@+id/login_win_update_pas_but"
        android:layout_alignEnd="@+id/login_win_update_pas_but"
        android:background="@color/colorAlpha1"
        android:visibility="gone" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:id="@+id/login_win_login_but"
        android:background="@drawable/icon_button1"
        android:layout_below="@+id/login_win_paw_et"
        android:layout_alignRight="@+id/imageView12"
        android:layout_alignEnd="@+id/imageView12"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登陆"
        android:id="@+id/textView25"
        android:layout_alignTop="@+id/imageView12"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp"
        android:textColor="@color/colorfont"
        android:textSize="30dp"
        android:enabled="false"
        android:clickable="false"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/login_win_user_et"
        android:layout_below="@+id/textView25"
        android:layout_centerHorizontal="true"
        android:background="@drawable/shurukuang1"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="15dp"
        android:textColor="@color/colorfont"
        android:gravity="center"
        android:maxLength="11"
        android:maxLines="1"
        android:hint="填写手机号码"
        android:textColorHint="@color/textcolorhint" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView19"
        android:layout_below="@+id/login_win_user_et"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="80dp"
        android:layout_marginRight="80dp"
        android:background="@drawable/icon_line5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/login_win_paw_et"
        android:background="@drawable/shurukuang1"
        android:layout_below="@+id/login_win_user_et"
        android:layout_alignLeft="@+id/login_win_user_et"
        android:layout_alignStart="@+id/login_win_user_et"
        android:layout_alignRight="@+id/login_win_user_et"
        android:layout_alignEnd="@+id/login_win_user_et"
        android:layout_marginTop="20dp"
        android:textColor="@color/colorfont"
        android:maxLength="12"
        android:maxLines="1"
        android:gravity="center"
        android:hint="填写密码"
        android:textColorHint="@color/textcolorhint" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登陆"
        android:id="@+id/textView27"
        android:textSize="30dp"
        android:gravity="center"
        android:textColor="@color/colorWhite"
        android:clickable="false"
        android:layout_above="@+id/textView28"
        android:layout_alignLeft="@+id/login_win_login_but"
        android:layout_alignStart="@+id/login_win_login_but"
        android:layout_alignRight="@+id/imageView12"
        android:layout_alignEnd="@+id/imageView12"
        android:editable="false"
        android:background="@color/colorAlpha1"
        android:layout_marginBottom="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="立即注册"
        android:id="@+id/textView28"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@color/regfontcolor"
        android:clickable="false"
        android:layout_alignBottom="@+id/imageView12"
        android:layout_alignLeft="@+id/login_win_login_but"
        android:layout_alignStart="@+id/login_win_login_but"
        android:layout_alignRight="@+id/imageView12"
        android:layout_alignEnd="@+id/imageView12"
        android:editable="false"
        android:background="@color/colorAlpha1" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/textView11"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#28020202"
        android:id="@+id/linearLayout8" >

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/login_return_but"
            android:background="@drawable/abc_ic_ab_back_mtrl_am_alpha"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="15dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/textView45"
            android:textColor="@color/colorWhite"
            android:textSize="20dp"
            android:layout_below="@+id/linearLayout7"
            android:layout_toRightOf="@+id/imageView22"
            android:layout_toEndOf="@+id/imageView22"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:layout_marginRight="15dp"
            android:text="@string/app_name" />
    </LinearLayout>

</RelativeLayout>